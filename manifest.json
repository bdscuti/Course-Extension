{
  "manifest_version": 3,
  "name": "Course Extension",
  "version": "0.1",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["course_jsons/*.json"],
      "matches": ["<all_urls>"]
    }
  ]
}