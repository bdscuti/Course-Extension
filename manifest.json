{
  "manifest_version": 3,
  "name": "Course Extension",
  "version": "1.0",
  "permissions": ["storage"],
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["courses_2026_spring.json"],
      "matches": ["<all_urls>"]
    }
  ]
}